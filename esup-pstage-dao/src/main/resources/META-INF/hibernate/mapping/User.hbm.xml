<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="org.esupportail.pstage.domain.beans">

	<class name="User" table="b_user" lazy="false"> 
		<cache usage="nonstrict-read-write" />
		<id name="id">
			<generator class="assigned" />
		</id> 
		<property name="displayName" column="disp_name" />
		<property name="admin" column="admi" not-null="true" />
		<property name="language" column="lang" />
	</class>

</hibernate-mapping>


