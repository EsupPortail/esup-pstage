<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<!-- 
		/properties/init/init.xml
		The configuration file for the initialization of the application.
		This file is referenced by /properties/applicationContext.xml.
	

	<bean 
		id="versionningService"
		class="org.esupportail.pstage.services.application.VersionningServiceImpl"
		parent="abstractDomainAwareBean"
		>
		<property name="firstAdministratorId" value="${init.firstAdministratorId}" />
	</bean>-->
	
	<bean
        id="versionningService"
        class="org.esupportail.commons.services.application.VoidVersionningServiceImpl"
        >
        <description>
            This bean is used to manage the initialization and the versionning
            of the application.
        </description>
    </bean>
	

</beans>
